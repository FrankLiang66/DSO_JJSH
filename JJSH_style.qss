QWidget
{
    selection-background-color:#3daee9;
    selection-color: #eff0f1;
    background-clip: border;
    border-image: none;
    border: 0px transparent black;
    outline: 0;
}

QWidget#widget_title
{
    color: #e7e6e6;
    background-color: #203864;    
    max-height: 72px;
    min-height: 72px;
}

QWidget#widget_title QPushButton
{
    color: transparent;
    background-color: transparent;   
	max-width: 25px;
    min-width: 25px;
    max-height: 25px;
    min-height: 25px;
}

QWidget#widget_main_nav
{
    color: #eff0f1;
    background-color: #203864;  
	max-height: 38px;
    min-height: 38px;
}

QWidget#widget_main_nav QPushButton
{
    font-size: 12pt;  
	qproperty-iconSize: 30px 30px;
	max-width: 150px;
    min-width: 150px;
}

QWidget#widget_sub_nav
{
    color: #eff0f1;
    background-color: #001239;
	max-width: 140px;
    min-width: 140px;
    min-height: 600px;
}

QWidget#widget_sub_nav QPushButton
{
	max-height: 25px;
    min-height: 25px;
}

QWidget#widget_mainWindow
{
    color: #eff0f1;
    background-color: #001239;
}

QPushButton 
{
    font-family: "Microsoft YaHei"; 
    color: #deebf7;
    font-size: 12pt; 
}

.QPushButton:disabled
{
    border-width: 1px;
    border-color: #c8c8c8;
    border-style: solid;
    padding-top: 2px;
    padding-bottom: 2px;
    padding-left: 4px;
    padding-right: 4px;
    border-radius: 2px;
    color: #c8c8c8;
	background-color: #7c7c9f;
}

.QPushButton:hover
{
    border-width: 1px;
    border-color: #53c0ef;
    border-style: solid;
    color: #53c0ef;
}

.QPushButton:pressed
{
    background-color: #4F6FA2;
    padding-top: -15px;
    padding-bottom: -17px;
    border-radius: 2px;
}

QPushButton#pushButton_minimize
{
    border-image: url(":/img/images/minus.png");
}

QPushButton#pushButton_maximize
{
    border-image: url(":/img/images/maximize.png");
}

QPushButton#pushButton_close
{
    border-image: url(":/img/images/close.png");
}

QPushButton#pushButton_main_home
{
    qproperty-icon: url(":/img/images/home.png") off, url(":/img/images/home.png") on;
}

QPushButton#pushButton_main_model
{
    qproperty-icon: url(":/img/images/design.png") off, url(":/img/images/design.png") on;
}

QPushButton#pushButton_main_monitor
{
    qproperty-icon: url(":/img/images/vigilance.png") off, url(":/img/images/vigilance.png") on;
}

QPushButton#pushButton_main_optimize
{
    qproperty-icon: url(":/img/images/pie-chart.png") off, url(":/img/images/pie-chart.png") on;
}

QPushButton#pushButton_main_tool
{
    qproperty-icon: url(":/img/images/settings.png") off, url(":/img/images/settings.png") on;
	max-width: 80px;
    min-width: 80px;
}

QPushButton#pushButton_main_help
{
    qproperty-icon: url(":/img/images/info.png") on;  
	max-width: 80px;
    min-width: 80px;
}


